{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": true,
   "logging": false,
   "entities": [
      "database/entity/**/*.ts"
   ],
   "migrations": [
      "database/migration/**/*.ts"
   ],
   "subscribers": [
      "database/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "database/entity",
      "migrationsDir": "database/migration",
      "subscribersDir": "database/subscriber"
   }
}